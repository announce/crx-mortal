{
  "manifest_version": 3,
  "name": "crx-mortal",
  "description": "This extension helps Mahjong players spot their less optimal moves",
  "version": "1.0.1",
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mjai.ekyu.moe/*"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ]
}
